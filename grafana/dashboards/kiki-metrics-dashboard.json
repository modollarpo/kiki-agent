{
  "dashboard": {
    "title": "KIKI SyncBrainâ„¢ Metrics",
    "tags": ["kiki", "syncbrain", "prometheus", "billing"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "LTV Predictions (24h)",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(ltv_predictions_total[24h])",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Prediction Confidence",
        "type": "gauge",
        "targets": [
          {
            "expr": "ltv_prediction_confidence_score",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Payment Success Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "payment_success_rate",
            "legendFormat": "{{ provider }}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Integration Health Status",
        "type": "table",
        "targets": [
          {
            "expr": "integration_health_status",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 5,
        "title": "API Latency (P95)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, api_latency_milliseconds_bucket)",
            "legendFormat": "{{ endpoint }}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 6,
        "title": "Active Customers",
        "type": "stat",
        "targets": [
          {
            "expr": "active_customers",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 32}
      },
      {
        "id": 7,
        "title": "Total Revenue",
        "type": "stat",
        "targets": [
          {
            "expr": "total_revenue_micros / 1000000",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 32}
      },
      {
        "id": 8,
        "title": "Circuit Breaker Trips",
        "type": "graph",
        "targets": [
          {
            "expr": "increase(circuit_breaker_trips_total[1h])",
            "legendFormat": "{{ service }}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 36}
      },
      {
        "id": 9,
        "title": "Margin Improvement %",
        "type": "graph",
        "targets": [
          {
            "expr": "margin_improvement_percentage",
            "legendFormat": "{{ client_id }}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 44}
      },
      {
        "id": 10,
        "title": "Audit Trail Size",
        "type": "stat",
        "targets": [
          {
            "expr": "audit_trail_records_total",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 52}
      },
      {
        "id": 11,
        "title": "System Uptime",
        "type": "stat",
        "targets": [
          {
            "expr": "ai_brain_uptime_seconds / 3600",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 52}
      }
    ],
    "schemaVersion": 27,
    "style": "dark",
    "templating": {
      "list": []
    },
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "weekStart": "monday"
  },
  "overwrite": true
}
